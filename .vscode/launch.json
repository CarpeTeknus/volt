  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Volt Secrets Service - Loki",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/main.ts",
        "-d",
        "debug.log"
      ],
      "env": {},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Volt Secrets Service - Loki, HTTPS, OAuth",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/main.ts",
        "--cert",
        "${workspaceFolder}/tests/server.pem",
        "--key",
        "${workspaceFolder}/tests/server-key.pem",
        "-d",
        "debug.log",
        "--oauth",
        "basic",
      ],
      "env": {},
      "outputCapture": "std"
    },
  ]
}